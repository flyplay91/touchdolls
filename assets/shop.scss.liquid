.fancybox-skin {    
  z-index: 99999999 !important;
}
.fancybox-outer, .fancybox-inner {
    position: relative;
    z-index: 9999999999999 !important;
}
#AccessibleNav a {
  border-right: none;
  color:{{ settings.nav_clr }};
  font-size: 16px;
  letter-spacing: 0.5px;
  padding: 0 10px;
  text-transform: uppercase;
  transition: all 500ms ease 0s;
}
.sca-qv-button-wrap {
    display: none !important;
}
/* tabs */
.new-tabs {
    display: inline-block;
    width: 100%;
  margin-top: 10px;
}
#productTabsNav.block {
   
    margin-bottom: 20px;
    display: inline-block;
    width: 100%;
      margin-left: 0;
}
#productTabsNav.nav-tabs > li {

    cursor: pointer;
    list-style: outside none none;
    padding: 0;
    border-radius: 5px;
    display: inline-block;
    width: 50%;
    border-bottom: 1px solid hsl(0, 0%, 80%);
}
#productTabsNav li a {
    text-align: center;
    font-size: 14px;
    line-height: 30px;
    background: #f0f0f0;
    margin: 0px;
    border: 1px solid #dddddd;
    letter-spacing: normal;
    padding: 0px 10px;
    transition: none;
    border-radius: 0px;
}
#product_tab_content .tab_content {
    color: #7B7E85;
    padding-left: 20px;
}
#productTabsNav li.active a {
    background: #fff;
}
#product_tab_content .tab-pane {
    display: none;
}
#product_tab_content .tab-pane.active {
    display: block;
}
#product_tab_content .tab-pane{
   max-height:none  ;
}
/*==================   Refine Acccordion ==========================*/
.accordion {
    background-color: #eee;
    color: #444;
    cursor: pointer;
    padding: 18px;
    width: 100%;
    border: none;
    text-align: left;
    outline: none;
    font-size: 15px;
      -webkit-transition: all 0.5s ease-in-out;
  -moz-transition: all 0.5s ease-in-out;
  -ms-transition: all 0.5s ease-in-out;
  transition: all 0.5s ease-in-out;
    
}

.accordion.active, .accordion:hover {
    background-color: #ddd; 
}

div.panel {
    padding: 0 18px;
    display: none;
    background-color: white;

}

div.panel.show {
    display: block;
}
